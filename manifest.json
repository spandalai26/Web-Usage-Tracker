{
    "manifest_version": 3,
    "name": "Web Usage Tracker",
    "version": "1.0",
    "description": "Tracks your web browsing patterns",
    "permissions": [
      "storage",
      "tabs",
      "webNavigation",
      "history"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }